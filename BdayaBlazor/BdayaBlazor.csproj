<Project Sdk="Microsoft.NET.Sdk.Razor">
	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<PackageIcon>Bdaya.png</PackageIcon>
		<PackageId>BdayaBlazor</PackageId>
		<Version>1.0.1</Version>
		<Authors>AhmedKhalil777</Authors>
		<Company>Bdaya-Dev</Company>
		<PackageDescription>A razor library for Bdaya Components</PackageDescription>
		<RepositoryUrl>https://github.com/Bdaya-Dev/BdayaBlazor</RepositoryUrl>
	</PropertyGroup>

	<ItemGroup>
		<SupportedPlatform Include="browser" />
	</ItemGroup>

	<ItemGroup>
		<None Include="Bdaya.png" Pack="true" PackagePath="\" />
	</ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="5.0.5" />
  </ItemGroup>

</Project>
